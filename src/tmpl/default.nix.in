# This is a minimal `default.nix` by yarn-plugin-nixify. You can customize it
# as needed, it will not be overwritten by the plugin.

{
  pkgs ? import <nixpkgs> { },
  plugins ? "${builtins.fetchTarball("https://github.com/stephank/yarn-plugin-nixify/archive/main.tar.gz")}/dist/yarn-plugin-nixify.js",
}:

pkgs.callPackage ./yarn-project.nix { inherit plugins; } { src = ./.; }
